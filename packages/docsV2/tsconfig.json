{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@content/*": ["./src/content/*"],
      "@/*": ["./src/*"],
    }
  }
}
